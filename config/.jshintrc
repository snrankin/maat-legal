{
    // JSHint Configuration File
    // See http://jshint.com/docs/ for more details

    "maxerr": 50,
    // Enforcing
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": false,
    "forin": true,
    "freeze": true,
    "immed": false,
    "latedef": false,
    "newcap": false,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": false,
    "undef": true,
    "unused": true,
    "strict": false,
    "maxparams": false,
    "maxdepth": false,
    "maxstatements": false,
    "maxcomplexity": false,
    "maxlen": false,
    "varstmt": false,

    // Relaxing
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": true,
    "esversion": 5,
    "moz": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": false,
    "multistr": false,
    "noyield": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "validthis": false,

    // Environments
    "browser": true,
    "browserify": false,
    "couch": false,
    "devel": true,
    "dojo": false,
    "jasmine": false,
    "jquery": true,
    "mocha": false,
    "mootools": false,
    "node": false,
    "nonstandard": true,
    "phantom": false,
    "prototypejs": false,
    "qunit": false,
    "rhino": false,
    "shelljs": false,
    "typed": false,
    "worker": false,
    "wsh": false,
    "yui": false,

    // Custom Globals
    "globals": {
        "google": true,
        "select2": true,
        "bsCustomFileInput": true,
        "makeFullHeight": true

    }
}